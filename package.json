{
  "name": "sql-with-mysql",
  "version": "1.0.0",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/henrikmathiesen/sql-with-mysql.git"
  },
  "scripts": {
    "start": "npm run ts && npm run serve:dev",
    "start:seed": "npm run ts && npm run serve:dev:seed",
    "start:delete": "npm run ts && npm run serve:dev:delete",
    "start:prod": "npm run ts && npm run serve:prod",
    "ts": "tsc",
    "clean-bld": "rimraf server/bld",
    "serve:dev": "node server/bld/index.js",
    "serve:dev:seed": "cross-env NODE_ENV=seed node server/bld/index.js",
    "serve:dev:delete": "cross-env NODE_ENV=delete node server/bld/index.js",
    "serve:prod": "cross-env NODE_ENV=production node server/bld/index.js"
  },
  "dependencies": {
    "body-parser": "1.17.2",
    "cross-env": "5.0.1",
    "express": "4.15.3",
    "mysql": "2.13.0"
  },
  "devDependencies": {
    "@types/body-parser": "1.16.4",
    "@types/express": "4.0.36",
    "@types/mysql": "0.0.33",
    "@types/node": "6.0.78",
    "rimraf": "2.6.1",
    "typescript": "2.3.4"
  }
}
